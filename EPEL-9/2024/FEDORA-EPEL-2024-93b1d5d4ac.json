						"creation_ts": "2022-04-08 20:41:44 +0000",
						"short_desc": "Can't install icedax on EPEL9",
						"delta_ts": "2024-01-10 14:55:55 +0000",
						"bug_status": "NEW",
						"priority": "unspecified",
