			"security": true,
			"title": "exiv2: embedded copy of exempi should be compiled with BanAllEntityUsage",
			"bugzilla": {
				"bug_id": "888769",
				"creation_ts": "2012-12-19 13:04:23 +0000",
				"short_desc": "exiv2: embedded copy of exempi should be compiled with BanAllEntityUsage",
				"delta_ts": "2018-04-11 11:20:59 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"keywords": "Security",
				"priority": "unspecified",
				"bug_severity": "unspecified",
				"depends_on": [
					"888768"
				],
