			"title": "CVE-2024-33871 ghostscript: OPVP device arbitrary code execution via custom Driver library",
			"bugzilla": {
				"bug_id": "2283508",
				"alias": "CVE-2024-33871",
				"creation_ts": "2024-05-27 14:40:53 +0000",
				"short_desc": "CVE-2024-33871 ghostscript: OPVP device arbitrary code execution via custom Driver library",
				"delta_ts": "2024-05-29 09:14:45 +0000",
				"bug_status": "NEW",
				"keywords": "Security",
				"priority": "high",
				"bug_severity": "high",
				"depends_on": [
