				"depends_on": [
					"2293788"
				],
				"blocked": [
					{
						"bug_id": "2293942",
						"alias": "CVE-2024-39331",
						"creation_ts": "2024-06-24 13:38:43 +0000",
						"short_desc": "CVE-2024-39331 emacs: org-link-expand-abbrev: Do not evaluate arbitrary unsafe Elisp code",
						"delta_ts": "2024-07-03 14:35:19 +0000",
						"bug_status": "NEW",
						"keywords": "Security",
						"priority": "medium",
						"bug_severity": "medium",
						"depends_on": [
							"2293944"
						],
						"blocked": [
							{
								"bug_id": "2293945",
