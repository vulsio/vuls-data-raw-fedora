				"creation_ts": "2024-08-22 19:20:34 +0000",
				"short_desc": "CVE-2024-8088 python: cpython: From NVD collector",
				"delta_ts": "2024-08-28 18:51:56 +0000",
				"bug_status": "NEW",
				"keywords": "Security",
				"priority": "medium",
				"bug_severity": "medium",
