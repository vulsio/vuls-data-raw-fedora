						"creation_ts": "2024-03-04 14:37:30 +0000",
						"short_desc": "CVE-2024-2002 libdwarf: crashes randomly on fuzzed object",
						"delta_ts": "2024-04-19 08:06:36 +0000",
						"bug_status": "NEW",
						"keywords": "Security",
