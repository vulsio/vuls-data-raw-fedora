			"title": "CVE-2021-40403 gerbv:pick-and-place rotation parsing use of uninitialized variable vulnerability",
			"bugzilla": {
				"bug_id": "2051389",
				"alias": "CVE-2021-40403",
				"creation_ts": "2022-02-07 07:48:45 +0000",
				"short_desc": "CVE-2021-40403 gerbv:pick-and-place rotation parsing use of uninitialized variable vulnerability",
				"delta_ts": "2022-02-07 08:14:32 +0000",
				"bug_status": "CLOSED",
				"resolution": "UPSTREAM",
				"keywords": "Security",
				"priority": "medium",
				"bug_severity": "medium",
				"depends_on": [
