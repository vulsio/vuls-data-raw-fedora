			"security": true,
			"title": "CVE-2022-1231 plantuml: Stored XSS in the context of the diagram embedder [fedora-all]",
			"bugzilla": {
				"bug_id": "2076163",
				"creation_ts": "2022-04-18 06:02:26 +0000",
				"short_desc": "CVE-2022-1231 plantuml: Stored XSS in the context of the diagram embedder [fedora-all]",
				"delta_ts": "2022-05-07 04:26:41 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"keywords": "Reopened, Security, SecurityTracking",
				"priority": "medium",
				"bug_severity": "medium",
				"blocked": [
					{
						"bug_id": "2076162",
						"alias": "CVE-2022-1231",
						"creation_ts": "2022-04-18 06:02:19 +0000",
						"short_desc": "CVE-2022-1231 plantuml: Stored XSS in the context of the diagram embedder",
						"delta_ts": "2022-04-18 08:56:56 +0000",
						"bug_status": "CLOSED",
						"resolution": "UPSTREAM",
						"keywords": "Security",
						"priority": "medium",
