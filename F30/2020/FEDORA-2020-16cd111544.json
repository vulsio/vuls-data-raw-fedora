			"parent": true,
			"security": true,
			"title": "CVE-2020-11100 haproxy: malformed HTTP/2 requests can lead to out-of-bounds writes",
			"bugzilla": {
				"bug_id": "1819111",
				"alias": "CVE-2020-11100",
				"creation_ts": "2020-03-31 08:48:59 +0000",
				"short_desc": "CVE-2020-11100 haproxy: malformed HTTP/2 requests can lead to out-of-bounds writes",
				"delta_ts": "2021-02-16 20:22:08 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
