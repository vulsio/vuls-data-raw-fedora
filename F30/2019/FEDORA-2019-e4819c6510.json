			"security": true,
			"title": "CVE-2019-16163 oniguruma: stack exhaustion in regcomp.c because of recursion in regparse.c [fedora-all]",
			"bugzilla": {
				"bug_id": "1768998",
				"creation_ts": "2019-11-05 18:08:48 +0000",
				"short_desc": "CVE-2019-16163 oniguruma: stack exhaustion in regcomp.c because of recursion in regparse.c [fedora-all]",
				"delta_ts": "2019-11-21 02:02:13 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"keywords": "Security, SecurityTracking",
				"priority": "medium",
				"bug_severity": "medium",
				"blocked": [
					{
						"bug_id": "1768997",
						"alias": "CVE-2019-16163",
						"creation_ts": "2019-11-05 18:08:36 +0000",
						"short_desc": "CVE-2019-16163 oniguruma: Stack exhaustion in regcomp.c because of recursion in regparse.c",
						"delta_ts": "2024-03-27 08:21:57 +0000",
						"bug_status": "CLOSED",
						"resolution": "ERRATA",
						"keywords": "Security",
						"priority": "medium",
						"bug_severity": "medium",
						"depends_on": [
							"1768998",
							"1768999",
							"1769000",
							"1772752",
							"1772753",
							"1772754",
							"1775402",
							"1775797",
