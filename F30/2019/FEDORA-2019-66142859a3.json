			"title": "CVE-2019-5953 wget: Buffer overflow vulnerability [fedora-all]",
			"bugzilla": {
				"bug_id": "1696738",
				"creation_ts": "2019-04-05 14:25:40 +0000",
				"short_desc": "CVE-2019-5953 wget: do_conversion() heap-based buffer overflow vulnerability [fedora-all]",
				"delta_ts": "2019-04-26 09:38:39 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
