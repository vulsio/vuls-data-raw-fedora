			"bugzilla": {
				"bug_id": "1652127",
				"alias": "CVE-2018-19387",
				"creation_ts": "2018-11-21 15:51:43 +0000",
				"short_desc": "CVE-2018-19387 tmux: NULL Pointer Dereference in format_cb_pane_tabs in format.c",
				"delta_ts": "2021-02-16 22:44:52 +0000",
				"bug_status": "CLOSED",
				"resolution": "NOTABUG",
				"keywords": "Security",
				"priority": "low",
				"bug_severity": "low",
				"depends_on": [
