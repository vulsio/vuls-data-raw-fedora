	"bugs": [
		{
			"bug_id": 1870290,
			"parent": true,
			"security": true,
			"title": "CVE-2020-24370 lua: segmentation fault in getlocal and setlocal functions in ldebug.c",
			"bugzilla": {
				"bug_id": "1870290",
				"alias": "CVE-2020-24370",
				"creation_ts": "2020-08-19 16:52:43 +0000",
				"short_desc": "CVE-2020-24370 lua: segmentation fault in getlocal and setlocal functions in ldebug.c",
				"delta_ts": "2021-11-09 23:23:16 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"keywords": "Security",
				"priority": "low",
				"bug_severity": "low",
				"depends_on": [
					"1880445"
				],
				"blocked": [
					{
						"bug_id": "1870295",
						"error": "NotPermitted"
					}
				],
				"external_bugs": {
					"text": "RHSA-2021:4510",
					"name": "Red Hat Product Errata"
				},
				"long_desc": [
					{
						"isprivate": "0",
						"commentid": "14216261",
						"comment_count": "0",
						"who": {
							"text": "gsuckevi",
							"name": "Guilherme de Almeida Suckevicz"
						},
						"bug_when": "2020-08-19 16:52:43 +0000",
						"thetext": "ldebug.c in Lua 5.4.0 allows a negation overflow and segmentation fault in getlocal and setlocal, as demonstrated by getlocal(3,2^31).\n\nReference:\nhttp://lua-users.org/lists/lua-l/2020-07/msg00324.html\n\nUpstream commit:\nhttps://github.com/lua/lua/commit/a585eae6e7ada1ca9271607a4f48dfb17868ab7b"
					},
					{
						"isprivate": "0",
						"commentid": "14216506",
						"comment_count": "1",
						"who": {
							"text": "spotrh",
							"name": "Tom \"spot\" Callaway"
						},
						"bug_when": "2020-08-19 18:45:49 +0000",
						"thetext": "Update pending for F31/F32, built for F33/F34."
					},
					{
						"isprivate": "0",
						"commentid": "15668925",
						"comment_count": "3",
						"who": {
							"text": "errata-xmlrpc",
							"name": "errata-xmlrpc"
						},
						"bug_when": "2021-11-09 19:02:36 +0000",
						"thetext": "This issue has been addressed in the following products:\n\n  Red Hat Enterprise Linux 8\n\nVia RHSA-2021:4510 https://access.redhat.com/errata/RHSA-2021:4510"
					},
					{
						"isprivate": "0",
						"commentid": "15671030",
						"comment_count": "4",
						"who": {
							"text": "prodsec-dev",
							"name": "Product Security DevOps Team"
						},
						"bug_when": "2021-11-09 23:23:15 +0000",
						"thetext": "This bug is now closed. Further updates for individual products will be reflected on the CVE page(s):\n\nhttps://access.redhat.com/security/cve/cve-2020-24370"
					}
				]
			}
		}
