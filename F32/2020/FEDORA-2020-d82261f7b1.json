	"bugs": [
		{
			"bug_id": 1900810,
			"parent": true,
			"security": true,
			"title": "CVE-2020-20739 vips: uninitialized variable in im_vips2dz function in im_vips2dz.c may leak remote server path or stack address",
			"bugzilla": {
				"bug_id": "1900810",
				"alias": "CVE-2020-20739",
				"creation_ts": "2020-11-23 18:37:40 +0000",
				"short_desc": "CVE-2020-20739 vips: uninitialized variable in im_vips2dz function in im_vips2dz.c may leak remote server path or stack address",
				"delta_ts": "2020-12-10 21:24:23 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"keywords": "Security",
				"priority": "medium",
				"bug_severity": "medium",
				"long_desc": [
					{
						"isprivate": "0",
						"commentid": "14533412",
						"comment_count": "0",
						"who": {
							"text": "gsuckevi",
							"name": "Guilherme de Almeida Suckevicz"
						},
						"bug_when": "2020-11-23 18:37:40 +0000",
						"thetext": "im_vips2dz in /libvips/libvips/deprecated/im_vips2dz.c in libvips before 8.8.2 has an uninitialized variable which may cause the leakage of remote server path or stack address.\n\nReference:\nhttps://github.com/libvips/libvips/issues/1419\n\nUpstream patch:\nhttps://github.com/libvips/libvips/commit/2ab5aa7bf515135c2b02d42e9a72e4c98e17031a"
					},
					{
						"isprivate": "0",
						"commentid": "14552206",
						"comment_count": "1",
						"who": {
							"text": "bgilbert",
							"name": "Benjamin Gilbert"
						},
						"bug_when": "2020-11-30 03:43:09 +0000",
						"thetext": "Affects F32, but not F33 or higher.  Appears to be low severity."
					},
					{
						"isprivate": "0",
						"commentid": "14575097",
						"comment_count": "2",
						"who": {
							"text": "bgilbert",
