					},
					{
						"isprivate": "0",
						"commentid": "15074977",
						"comment_count": "9",
						"who": {
							"text": "errata-xmlrpc",
							"name": "errata-xmlrpc"
						},
						"bug_when": "2021-05-18 14:15:54 +0000",
