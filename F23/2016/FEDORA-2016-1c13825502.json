			"title": "CVE-2013-5653 ghostscript: getenv and filenameforall ignore -dSAFER",
			"bugzilla": {
				"bug_id": "1380327",
				"alias": "CVE-2013-5653",
				"creation_ts": "2016-09-29 10:03:47 +0000",
				"short_desc": "CVE-2013-5653 ghostscript: getenv and filenameforall ignore -dSAFER",
				"delta_ts": "2019-09-29 13:57:05 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"keywords": "Security",
				"priority": "medium",
				"bug_severity": "medium",
				"depends_on": [
					"1390299",
					"1390300",
					"1390301",
					"1390302",
					"1390486"
				],
				"blocked": [
					{
						"bug_id": "1380329",
						"error": "NotPermitted"
					}
				],
