			"security": true,
			"title": "CVE-2016-7076 sudo: noexec bypass via wordexp() [fedora-all]",
			"bugzilla": {
				"bug_id": "1389496",
				"creation_ts": "2016-10-27 18:05:27 +0000",
				"short_desc": "CVE-2016-7076 sudo: noexec bypass via wordexp() [fedora-all]",
				"delta_ts": "2016-11-25 07:21:57 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"keywords": "Security, SecurityTracking",
				"priority": "medium",
				"bug_severity": "medium",
				"blocked": [
					{
						"bug_id": "1384982",
						"alias": "CVE-2016-7076",
						"creation_ts": "2016-10-14 13:41:49 +0000",
						"short_desc": "CVE-2016-7076 sudo: noexec bypass via wordexp()",
						"delta_ts": "2021-12-10 14:45:40 +0000",
						"bug_status": "CLOSED",
						"resolution": "ERRATA",
						"keywords": "Security",
						"priority": "medium",
						"bug_severity": "medium",
						"depends_on": [
							"1389496",
							"1391937",
							"1391938",
							"1391939",
							"1391940"
						],
						"blocked": [
							{
								"bug_id": "1372831",
								"error": "NotPermitted"
