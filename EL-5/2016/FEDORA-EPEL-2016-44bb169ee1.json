			"title": "collectd: Unchecked error conditions of gcry_control",
			"bugzilla": {
				"bug_id": "1361062",
				"creation_ts": "2016-07-28 09:41:05 +0000",
				"short_desc": "collectd: Unchecked error conditions of gcry_control",
				"delta_ts": "2019-09-29 13:54:22 +0000",
				"bug_status": "CLOSED",
				"resolution": "WONTFIX",
				"keywords": "Security",
				"priority": "low",
				"bug_severity": "low",
				"depends_on": [
					"1361063",
					"1361064",
					"1366931",
					"1366932",
					"1366933",
					"1366934"
				],
