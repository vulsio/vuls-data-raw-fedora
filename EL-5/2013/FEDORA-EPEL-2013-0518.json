						"short_desc": "CVE-2012-4066 eucalyptus: Walrus Request Manipulation Vulnerability",
						"delta_ts": "2019-09-29 13:01:09 +0000",
						"bug_status": "CLOSED",
						"resolution": "ERRATA",
						"keywords": "Security",
						"priority": "medium",
