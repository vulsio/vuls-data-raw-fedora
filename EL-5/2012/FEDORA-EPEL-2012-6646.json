			"title": "CVE-2012-3455 Koffice: DOC file rendering buffer overflow",
			"bugzilla": {
				"bug_id": "846097",
				"alias": "CVE-2012-3455",
				"creation_ts": "2012-08-06 19:49:08 +0000",
				"short_desc": "CVE-2012-3455 Koffice: DOC file rendering buffer overflow",
				"delta_ts": "2019-09-29 12:54:31 +0000",
				"bug_status": "CLOSED",
				"resolution": "UPSTREAM",
