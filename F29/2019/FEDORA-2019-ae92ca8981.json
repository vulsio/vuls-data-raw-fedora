								"comment_count": "2",
								"who": {
									"text": "scorneli",
									"name": "Stefan Cornelius"
								},
								"bug_when": "2018-12-05 13:41:18 +0000",
								"thetext": "Looks like this was introduced by https://github.com/libjpeg-turbo/libjpeg-turbo/commit/aa7459050d7a50e1d8a99488902d41fbc118a50f"
							},
							{
								"isprivate": "0",
