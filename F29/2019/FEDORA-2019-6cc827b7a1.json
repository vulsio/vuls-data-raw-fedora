	"autokarma": true,
	"bugs": [
		{
			"bug_id": 1672626,
			"parent": true,
			"security": true,
			"title": "CVE-2018-20751 podofo: null pointer dereference in crop_page function",
			"bugzilla": {
				"bug_id": "1672626",
				"alias": "CVE-2018-20751",
				"creation_ts": "2019-02-05 13:30:30 +0000",
				"short_desc": "CVE-2018-20751 podofo: null pointer dereference in crop_page function",
				"delta_ts": "2021-10-27 03:22:30 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"keywords": "Security",
				"priority": "medium",
				"bug_severity": "medium",
				"long_desc": [
					{
						"isprivate": "0",
						"commentid": "12422410",
						"comment_count": "0",
						"who": {
							"text": "darunesh",
							"name": "Dhananjay Arunesh"
						},
						"bug_when": "2019-02-05 13:30:30 +0000",
						"thetext": "An issue was discovered in crop_page in PoDoFo 0.9.6. For a crafted PDF document, pPage->GetObject()->GetDictionary().AddKey(PdfName(\"MediaBox\"),var) can be problematic due to the function GetObject() being called for the pPage NULL pointer object. The value of pPage at this point is 0x0, which causes a NULL pointer dereference.\n\nReferences:\nhttps://research.loginsoft.com/bugs/null-pointer-dereference-vulnerability-in-crop_page-podofo-0-9-6/\nhttps://sourceforge.net/p/podofo/tickets/33/\nhttps://sourceforge.net/projects/podofo/"
					}
				]
			}
		}
	],
