			"security": true,
			"title": "CVE-2022-48337 emacs: command execution via shell metacharacters [fedora-all]",
			"bugzilla": {
				"bug_id": "2171992",
				"creation_ts": "2023-02-21 06:24:20 +0000",
				"short_desc": "CVE-2022-48337 emacs: command execution via shell metacharacters [fedora-all]",
				"delta_ts": "2023-10-14 01:26:32 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"keywords": "Security, SecurityTracking",
				"priority": "medium",
				"bug_severity": "medium",
				"blocked": [
					{
