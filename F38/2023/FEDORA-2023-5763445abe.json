						"short_desc": "CVE-2022-48337 emacs: command execution via shell metacharacters",
						"delta_ts": "2024-03-19 17:29:30 +0000",
						"bug_status": "NEW",
						"keywords": "Security",
						"priority": "medium",
						"bug_severity": "medium",
						"depends_on": [
							"2171992",
							"2175188",
							"2175189",
