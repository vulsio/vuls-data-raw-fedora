						"short_desc": "CVE-2023-40569 freerdp: Out-of-bounds write in the `progressive_decompress` function due to incorrect calculations",
						"delta_ts": "2024-04-30 09:48:45 +0000",
						"bug_status": "NEW",
						"keywords": "Security",
						"priority": "medium",
						"bug_severity": "medium",
						"depends_on": [
							"2236653",
							"2236654",
							"2236655"
						],
