			"title": "Double free in MIT Kerberos 1.21",
			"bugzilla": {
				"bug_id": "2229113",
				"creation_ts": "2023-08-04 08:31:01 +0000",
				"short_desc": "Double free in MIT Kerberos 1.21",
				"delta_ts": "2023-08-16 12:54:39 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"keywords": "Regression, Triaged",
