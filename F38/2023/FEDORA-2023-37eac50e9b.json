			"security": true,
			"title": "CVE-2023-28322 curl: more POST-after-PUT confusion [fedora-all]",
			"bugzilla": {
				"bug_id": "2209338",
				"creation_ts": "2023-05-23 14:26:26 +0000",
				"short_desc": "CVE-2023-28322 curl: more POST-after-PUT confusion [fedora-all]",
				"delta_ts": "2023-06-08 02:00:02 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"keywords": "Security, SecurityTracking",
