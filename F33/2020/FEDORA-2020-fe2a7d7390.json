								"isprivate": "0",
								"commentid": "14332695",
								"comment_count": "1",
								"who": {
									"text": "gsuckevi",
									"name": "Guilherme de Almeida Suckevicz"
								},
								"bug_when": "2020-09-29 18:00:09 +0000",
								"thetext": "Created ruby tracking bugs for this issue:\n\nAffects: fedora-all [bug 1883624]"
							},
