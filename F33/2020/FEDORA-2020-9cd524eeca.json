						],
						"external_bugs": {
							"text": "RHSA-2021:4251",
							"name": "Red Hat Product Errata"
						},
						"long_desc": [
							{
								"isprivate": "0",
								"commentid": "14547104",
								"comment_count": "0",
								"who": {
									"text": "mkaplan",
									"name": "Michael Kaplan"
								},
								"bug_when": "2020-11-26 15:01:35 +0000",
								"thetext": "A heap-buffer overwrites error was discovered in lib/openjp2/mqc.c in\nOpenJPEG 2.3.1. The vulnerability causes an out-of-bounds write, which may\nlead to remote denial of service or possibly remote code execution."
							},
							{
								"isprivate": "0",
								"commentid": "14547112",
								"comment_count": "2",
								"who": {
									"text": "mkaplan",
									"name": "Michael Kaplan"
								},
								"bug_when": "2020-11-26 15:02:02 +0000",
								"thetext": "Created mingw-openjpeg2 tracking bugs for this issue:\n\nAffects: fedora-all [bug 1902001]\n\n\nCreated openjpeg2 tracking bugs for this issue:\n\nAffects: epel-all [bug 1901999]\nAffects: fedora-all [bug 1902000]"
							},
							{
								"isprivate": "0",
								"commentid": "14549392",
								"comment_count": "4",
								"who": {
									"text": "mkaplan",
									"name": "Michael Kaplan"
								},
								"bug_when": "2020-11-27 13:36:33 +0000",
								"thetext": "Acknowledgments:\n\nName: zodf0055980 (SQLab NCTU Taiwan)"
							},
							{
