					}
				],
				"long_desc": [
					{
						"isprivate": "0",
						"commentid": "14536318",
						"comment_count": "0",
						"who": {
							"text": "gsuckevi",
							"name": "Guilherme de Almeida Suckevicz"
						},
						"bug_when": "2020-11-24 13:41:15 +0000",
						"thetext": "A flaw was found in Linux-Pam in the way it handle empty passwords for non-existing users. When the user doesn't exist PAM try to authenticate with root and in the case of an empty password it successfully authenticate.\n\nReference:\nhttps://github.com/linux-pam/linux-pam/issues/284\n\nUpstream patch:\nhttps://github.com/linux-pam/linux-pam/pull/300"
					},
					{
						"isprivate": "0",
						"commentid": "14537937",
						"comment_count": "1",
						"who": {
							"text": "scorneli",
							"name": "Stefan Cornelius"
						},
						"bug_when": "2020-11-24 16:25:18 +0000",
						"thetext": "Introduced in version 1.5.0."
					},
					{
						"isprivate": "0",
						"commentid": "14538062",
						"comment_count": "2",
						"who": {
