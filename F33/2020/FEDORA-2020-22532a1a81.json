			"bugzilla": {
				"bug_id": "1901094",
				"alias": "CVE-2020-27780",
				"creation_ts": "2020-11-24 13:41:15 +0000",
				"short_desc": "CVE-2020-27780 pam: authentication bypass when the user doesn't exist and root password is blank",
				"delta_ts": "2021-02-10 15:25:55 +0000",
				"bug_status": "CLOSED",
				"resolution": "NOTABUG",
				"keywords": "Security",
				"priority": "medium",
				"bug_severity": "medium",
				"depends_on": [
