			"security": true,
			"title": "CVE-2021-3560 polkit: local privilege escalation using polkit_system_bus_name_get_creds_sync()",
			"bugzilla": {
				"bug_id": "1961710",
				"alias": "CVE-2021-3560",
				"creation_ts": "2021-05-18 14:32:58 +0000",
				"short_desc": "CVE-2021-3560 polkit: local privilege escalation using polkit_system_bus_name_get_creds_sync()",
				"delta_ts": "2023-10-09 12:38:00 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"keywords": "Security",
				"priority": "high",
				"bug_severity": "high",
