			"title": "CVE-2023-31047 python-django3: python-django: Potential bypass of validation when uploading multiple files using one form field [fedora-all]",
			"bugzilla": {
				"bug_id": "2196197",
				"creation_ts": "2023-05-08 09:28:59 +0000",
				"short_desc": "CVE-2023-31047 python-django3: python-django: Potential bypass of validation when uploading multiple files using one form field [fedora-all]",
				"delta_ts": "2023-05-16 01:28:36 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"keywords": "Security, SecurityTracking",
				"priority": "low",
				"bug_severity": "low",
				"blocked": [
					{
						"bug_id": "2192565",
						"alias": "CVE-2023-31047",
						"creation_ts": "2023-05-02 10:00:12 +0000",
						"short_desc": "CVE-2023-31047 python-django: Potential bypass of validation when uploading multiple files using one form field",
						"delta_ts": "2023-11-08 14:17:22 +0000",
						"bug_status": "CLOSED",
						"resolution": "ERRATA",
						"keywords": "Security",
						"priority": "medium",
						"bug_severity": "medium",
						"depends_on": [
							"2196195",
							"2196196",
							"2196197",
							"2196199",
							"2238376"
