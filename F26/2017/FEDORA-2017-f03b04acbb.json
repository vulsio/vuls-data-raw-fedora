			"title": "CVE-2017-1000116 mercurial: command injection on clients through malicious ssh URLs",
			"bugzilla": {
				"bug_id": "1479915",
				"alias": "CVE-2017-1000116",
				"creation_ts": "2017-08-09 18:07:49 +0000",
				"short_desc": "CVE-2017-1000116 mercurial: command injection on clients through malicious ssh URLs",
				"delta_ts": "2021-02-17 01:44:35 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"keywords": "Security",
				"priority": "high",
				"bug_severity": "high",
				"depends_on": [
					"1479968",
					"1479969",
					"1480455"
				],
