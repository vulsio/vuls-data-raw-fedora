				"priority": "low",
				"bug_severity": "low",
				"blocked": [
					{
						"bug_id": "1366239",
						"alias": "CVE-2016-6328",
						"creation_ts": "2016-08-11 11:07:25 +0000",
						"short_desc": "CVE-2016-6328 libexif: Integer overflow in parsing MNOTE entry data of the input file",
						"delta_ts": "2021-06-03 15:57:34 +0000",
						"bug_status": "CLOSED",
						"resolution": "WONTFIX",
						"keywords": "Security",
						"priority": "low",
