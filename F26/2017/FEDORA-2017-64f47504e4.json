	"bugs": [
		{
			"bug_id": 1464920,
			"parent": true,
			"security": true,
			"title": "CVE-2017-9772 ocaml: Insufficient sanitisation allows privilege escalation for setuid binaries",
			"bugzilla": {
				"bug_id": "1464920",
				"alias": "CVE-2017-9772",
				"creation_ts": "2017-06-26 08:57:44 +0000",
				"short_desc": "CVE-2017-9772 ocaml: Insufficient sanitisation allows privilege escalation for setuid binaries",
				"delta_ts": "2021-02-17 02:00:10 +0000",
				"bug_status": "CLOSED",
				"resolution": "NOTABUG",
				"keywords": "Security",
				"priority": "medium",
				"bug_severity": "medium",
				"depends_on": [
					"1464921"
				],
				"long_desc": [
					{
						"isprivate": "0",
						"commentid": "10539433",
						"comment_count": "0",
						"who": {
							"text": "anemec",
							"name": "Andrej Nemec"
						},
						"bug_when": "2017-06-26 08:57:44 +0000",
						"thetext": "\nInsufficient sanitisation in the OCaml compiler versions 4.04.0 and 4.04.1 allows external code to be executed with raised privilege in binaries marked as setuid, by setting the CAML_CPLUGINS, CAML_NATIVE_CPLUGINS, or CAML_BYTE_CPLUGINS environment variable.\n\nExternal References:\n\nhttps://sympa.inria.fr/sympa/arc/caml-list/2017-06/msg00094.html\n\nUpstream issue:\n\nhttps://caml.inria.fr/mantis/view.php?id=7557"
					},
					{
						"isprivate": "0",
						"commentid": "10539473",
						"comment_count": "1",
						"who": {
							"text": "anemec",
							"name": "Andrej Nemec"
						},
						"bug_when": "2017-06-26 08:58:34 +0000",
						"thetext": "Created ocaml tracking bugs for this issue:\n\nAffects: fedora-rawhide [bug 1464921]"
					},
					{
						"isprivate": "0",
						"commentid": "10539553",
						"comment_count": "2",
						"who": {
							"text": "rjones",
							"name": "Richard W.M. Jones"
						},
						"bug_when": "2017-06-26 09:16:32 +0000",
						"thetext": "Affects Fedora 26 (OCaml 4.04.0) as well."
					}
				]
			}
		}
	],
