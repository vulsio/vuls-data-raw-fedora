			"title": "electrum: Unprotected JSON-RPC interface",
			"bugzilla": {
				"bug_id": "1532554",
				"alias": "CVE-2018-1000022",
				"creation_ts": "2018-01-09 10:23:37 +0000",
				"short_desc": "CVE-2018-1000022 electrum: Unprotected JSON-RPC interface",
				"delta_ts": "2019-09-29 14:29:50 +0000",
				"bug_status": "CLOSED",
				"resolution": "CURRENTRELEASE",
				"keywords": "Security",
				"priority": "high",
				"bug_severity": "high",
				"depends_on": [
					"1532555"
				],
				"long_desc": [
					{
						"isprivate": "0",
						"commentid": "11167514",
						"comment_count": "0",
						"who": {
							"text": "anemec",
							"name": "Andrej Nemec"
						},
						"bug_when": "2018-01-09 10:23:37 +0000",
						"thetext": "\nIt was found that electrum before 3.0.4 allowed CORS for the JSON-RPC server. This allows malicious users to possibly access wallets via the local RPC ports.\n\nUpstream issue:\n\nhttps://github.com/spesmilo/electrum/issues/3374"
					},
					{
						"isprivate": "0",
						"commentid": "11167517",
						"comment_count": "1",
						"who": {
							"text": "anemec",
							"name": "Andrej Nemec"
						},
						"bug_when": "2018-01-09 10:23:56 +0000",
						"thetext": "Created electrum tracking bugs for this issue:\n\nAffects: fedora-26 [bug 1532555]"
					},
					{
						"isprivate": "0",
						"commentid": "11174115",
						"comment_count": "2",
						"who": {
							"text": "updates",
							"name": "Fedora Update System"
						},
						"bug_when": "2018-01-10 22:23:44 +0000",
						"thetext": "electrum-3.0.5-1.fc26 has been pushed to the Fedora 26 stable repository. If problems still persist, please make note of it in this bug report."
					},
					{
						"isprivate": "0",
						"commentid": "11229841",
						"comment_count": "3",
						"who": {
							"text": "hegjon",
							"name": "Jonny Heggheim"
						},
						"bug_when": "2018-01-29 20:54:53 +0000",
						"thetext": "This bug have been fixed in all supported versions of Fedora."
					}
				]
			}
