			"bugzilla": {
				"bug_id": "1559049",
				"creation_ts": "2018-03-21 15:21:35 +0000",
				"short_desc": "Test failures with latest ImageMagick updates",
				"delta_ts": "2018-04-02 13:01:31 +0000",
