						"long_desc": [
							{
								"isprivate": "0",
								"commentid": "7467633",
								"comment_count": "0",
								"who": {
									"text": "pmatouse",
									"name": "Petr Matousek"
								},
								"bug_when": "2014-09-10 16:59:19 +0000",
								"thetext": "A flaw was found in the way the termination condition in the associative array\ngarbage collection functionality was handled when used from the keys subsystem.\n\nA local unprivileged user could use this flaw to crash the system.\n\nIntroduced by:\nhttp://git.kernel.org/cgit/linux/kernel/git/torvalds/linux.git/commit/?id=b2a4df200d570b2c33a57e1ebfa5896e4bc81b69"
							},
							{
								"isprivate": "0",
								"commentid": "7467643",
								"comment_count": "1",
								"who": {
									"text": "pmatouse",
									"name": "Petr Matousek"
								},
								"bug_when": "2014-09-10 17:02:15 +0000",
								"thetext": "Created attachment 936266\nUpstream patch proposal"
							},
							{
								"isprivate": "0",
								"commentid": "7467647",
								"comment_count": "2",
								"who": {
									"text": "pmatouse",
									"name": "Petr Matousek"
								},
								"bug_when": "2014-09-10 17:03:31 +0000",
								"thetext": "Statement:\n\nThis issue does not affect versions of the Linux kernel as shipped with Red Hat Enterprise Linux 5, 6 and Red Hat Enterprise MRG 2. Future kernel updates for Red Hat Enterprise Linux 7 may address this issue."
							},
							{
								"isprivate": "0",
								"commentid": "7468420",
								"comment_count": "4",
								"who": {
									"text": "dhowells",
									"name": "David Howells"
								},
								"bug_when": "2014-09-10 21:32:01 +0000",
								"thetext": "The patch has been posted upstream: https://lkml.org/lkml/2014/9/10/788"
							},
							{
								"isprivate": "0",
								"commentid": "7479915",
								"comment_count": "5",
								"who": {
									"text": "dhowells",
									"name": "David Howells"
								},
								"bug_when": "2014-09-15 09:45:46 +0000",
								"thetext": "The patch has been applied upstream:\n\nhttp://git.kernel.org/cgit/linux/kernel/git/torvalds/linux.git/commit/?id=95389b08d93d5c06ec63ab49bd732b0069b7c35e"
							},
							{
								"isprivate": "0",
								"commentid": "7595367",
								"comment_count": "7",
								"who": {
									"text": "pmatouse",
									"name": "Petr Matousek"
