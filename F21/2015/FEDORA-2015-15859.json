				"resolution": "ERRATA",
				"keywords": "Security, SecurityTracking",
				"priority": "medium",
				"bug_severity": "medium",
				"blocked": [
					{
						"bug_id": "1260944",
						"alias": "CVE-2015-7697",
						"creation_ts": "2015-09-08 09:37:28 +0000",
						"short_desc": "CVE-2015-7696 CVE-2015-7697 unzip: Heap overflow and DoS in 6.0",
						"delta_ts": "2019-11-14 06:56:52 +0000",
						"bug_status": "CLOSED",
						"resolution": "WONTFIX",
						"keywords": "Security",
						"priority": "medium",
