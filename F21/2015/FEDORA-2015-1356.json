				"alias": "CVE-2015-0239",
				"creation_ts": "2015-01-27 16:59:16 +0000",
				"short_desc": "CVE-2015-0239 kernel: kvm: insufficient sysenter emulation when invoked from 16-bit code",
				"delta_ts": "2023-05-12 07:06:02 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"keywords": "Security",
				"priority": "low",
				"bug_severity": "low",
				"depends_on": [
					"1186449",
					"1186450",
					"1186451",
					"1186452",
					"1186453"
				],
