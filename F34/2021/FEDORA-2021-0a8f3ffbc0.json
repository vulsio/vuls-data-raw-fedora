			"parent": true,
			"security": true,
			"title": "CVE-2021-3426 python: information disclosure via pydoc",
			"bugzilla": {
				"bug_id": "1935913",
				"alias": "CVE-2021-3426",
				"creation_ts": "2021-03-05 19:20:02 +0000",
				"short_desc": "CVE-2021-3426 python: Information disclosure via pydoc",
				"delta_ts": "2024-06-14 00:40:01 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"keywords": "Security",
				"priority": "medium",
				"bug_severity": "medium",
