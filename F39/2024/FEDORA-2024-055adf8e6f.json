						"short_desc": "CVE-2024-45306 vim: heap-buffer-overflow in Vim",
						"delta_ts": "2024-09-03 20:52:00 +0000",
						"bug_status": "NEW",
						"keywords": "Security",
						"priority": "low",
						"bug_severity": "low",
