			"security": true,
			"title": "CVE-2024-2004 curl: Usage of disabled protocol [fedora-all]",
			"bugzilla": {
				"bug_id": "2271826",
				"creation_ts": "2024-03-27 13:17:44 +0000",
				"short_desc": "CVE-2024-2004 curl: Usage of disabled protocol [fedora-all]",
				"delta_ts": "2024-04-25 01:19:29 +0000",
