				"blocked": [
					{
						"bug_id": "2279632",
						"alias": "CVE-2024-34397",
						"creation_ts": "2024-05-07 19:59:58 +0000",
						"short_desc": "CVE-2024-34397 glib2: Signal subscription vulnerabilities",
						"delta_ts": "2024-07-10 14:24:11 +0000",
						"bug_status": "NEW",
						"keywords": "Security",
						"priority": "medium",
						"bug_severity": "medium",
						"depends_on": [
							"2279637",
							"2279638",
							"2279639",
							"2279640",
							"2279641",
							"2279642"
						],
						"blocked": [
							{
