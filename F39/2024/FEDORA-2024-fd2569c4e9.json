			"security": true,
			"title": "CVE-2024-34397 glib2: Signal subscription vulnerabilities [fedora-39]",
			"bugzilla": {
				"bug_id": "2279640",
				"creation_ts": "2024-05-07 20:06:29 +0000",
				"short_desc": "CVE-2024-34397 glib2: Signal subscription vulnerabilities [fedora-39]",
				"delta_ts": "2024-05-14 03:27:53 +0000",
