			"security": true,
			"title": "CVE-2016-8637 dracut: Local information disclosure of initramfs when early cpio is used [fedora-all]",
			"bugzilla": {
				"bug_id": "1392435",
				"creation_ts": "2016-11-07 13:53:34 +0000",
				"short_desc": "CVE-2016-8637 dracut: Local information disclosure of initramfs when early cpio is used [fedora-all]",
				"delta_ts": "2016-11-19 21:13:01 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"keywords": "Security, SecurityTracking",
				"priority": "medium",
				"bug_severity": "medium",
				"blocked": [
					{
						"bug_id": "1392056",
						"alias": "CVE-2016-8637",
						"creation_ts": "2016-11-04 16:49:46 +0000",
						"short_desc": "CVE-2016-8637 dracut: Local information disclosure of initramfs when early cpio is used",
						"delta_ts": "2021-02-17 03:04:47 +0000",
						"bug_status": "CLOSED",
						"resolution": "NOTABUG",
						"keywords": "Security",
						"priority": "medium",
