			"security": true,
			"title": "CVE-2016-10002 CVE-2016-10003 squid: various flaws [fedora-all]",
			"bugzilla": {
				"bug_id": "1405944",
				"creation_ts": "2016-12-19 09:11:40 +0000",
				"short_desc": "CVE-2016-10002 CVE-2016-10003 squid: various flaws [fedora-all]",
				"delta_ts": "2017-01-20 17:52:46 +0000",
				"bug_status": "CLOSED",
