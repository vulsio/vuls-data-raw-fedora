				"blocked": [
					{
						"bug_id": "1506532",
						"alias": "CVE-2017-12618",
						"creation_ts": "2017-10-26 09:39:32 +0000",
						"short_desc": "CVE-2017-12618 apr-util: Out-of-bounds access in corrupted SDBM database",
						"delta_ts": "2021-10-21 11:57:28 +0000",
						"bug_status": "CLOSED",
						"resolution": "ERRATA",
						"keywords": "Security",
						"priority": "medium",
						"bug_severity": "medium",
						"depends_on": [
							"1506535",
							"1506922",
							"1506923"
						],
						"blocked": [
