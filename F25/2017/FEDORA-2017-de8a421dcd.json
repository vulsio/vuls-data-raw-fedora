			"title": "CVE-2017-13089 wget: Stack-based buffer overflow in HTTP protocol handling",
			"bugzilla": {
				"bug_id": "1505444",
				"alias": "CVE-2017-13089",
				"creation_ts": "2017-10-23 15:08:44 +0000",
				"short_desc": "CVE-2017-13089 wget: Stack-based buffer overflow in HTTP protocol handling",
				"delta_ts": "2021-02-17 01:19:46 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"keywords": "Security",
				"priority": "high",
				"bug_severity": "high",
				"depends_on": [
					"1505488",
					"1505489",
					"1505490",
					"1505491",
					"1506720",
					"1561420"
				],
