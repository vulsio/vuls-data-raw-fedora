	"bugs": [
		{
			"bug_id": 2192126,
			"title": "CVE-2022-26562: zarafa: Missing account validation in ECPAMAuthenticateUser()",
			"bugzilla": {
				"bug_id": "2192126",
				"alias": "CVE-2022-26562",
				"creation_ts": "2023-04-29 19:14:12 +0000",
				"short_desc": "CVE-2022-26562: zarafa: Missing account validation in ECPAMAuthenticateUser()",
				"delta_ts": "2023-05-08 00:48:52 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"priority": "unspecified",
				"bug_severity": "medium",
				"long_desc": [
					{
						"isprivate": "0",
						"commentid": "17230876",
						"comment_count": "0",
						"who": {
							"text": "redhat-bugzilla",
							"name": "Robert Scheck"
						},
						"bug_when": "2023-04-29 19:14:12 +0000",
						"thetext": "An issue in provider/libserver/ECKrbAuth.cpp of Kopano-Core v11.0.2.51 contains an issue which allows attackers to authenticate even if the user account or password is expired.\n\nReferences:\n- https://cve.mitre.org/cgi-bin/cvename.cgi?name=CVE-2022-26562\n- https://bugs.debian.org/cgi-bin/bugreport.cgi?bug=1016973\n- https://jira.kopano.io/browse/KC-2021\n- https://github.com/Kopano-dev/kopano-core/blob/master/provider/libserver/ECKrbAuth.cpp#L137"
					},
					{
						"isprivate": "0",
						"commentid": "17230999",
						"comment_count": "1",
						"who": {
							"text": "redhat-bugzilla",
							"name": "Robert Scheck"
						},
						"bug_when": "2023-04-29 21:23:52 +0000",
						"thetext": "The security flaw was introduced between Zarafa 6.30.0 RC 1e (Subversion Revision 14802) and 6.30.8 Final (Subversion Revision 18345) in provider/libserver/ECPamAuth.cpp and also affects Kopano Core <= 11.0.2.51 in provider/libserver/ECKrbAuth.cpp."
					},
					{
						"isprivate": "0",
						"commentid": "17231048",
						"comment_count": "2",
						"who": {
							"text": "updates",
							"name": "Fedora Update System"
						},
						"bug_when": "2023-04-29 22:27:12 +0000",
						"thetext": "FEDORA-EPEL-2023-342b96903b has been submitted as an update to Fedora EPEL 7. https://bodhi.fedoraproject.org/updates/FEDORA-EPEL-2023-342b96903b"
					},
					{
						"isprivate": "0",
						"commentid": "17231119",
						"comment_count": "3",
						"who": {
							"text": "updates",
							"name": "Fedora Update System"
						},
						"bug_when": "2023-04-30 00:19:43 +0000",
						"thetext": "FEDORA-EPEL-2023-342b96903b has been pushed to the Fedora EPEL 7 testing repository.\n\nYou can provide feedback for this update here: https://bodhi.fedoraproject.org/updates/FEDORA-EPEL-2023-342b96903b\n\nSee also https://fedoraproject.org/wiki/QA:Updates_Testing for more information on how to test updates."
					},
					{
