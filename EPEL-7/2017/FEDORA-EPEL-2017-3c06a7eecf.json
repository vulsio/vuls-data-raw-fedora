				"bug_severity": "medium",
				"blocked": [
					{
						"bug_id": "1490858",
						"alias": "CVE-2017-14312",
						"creation_ts": "2017-09-12 11:48:45 +0000",
						"short_desc": "CVE-2017-14312 nagios: Incorrect file permissions leading to possible privilege escalation",
						"delta_ts": "2020-12-14 10:26:02 +0000",
						"bug_status": "CLOSED",
						"resolution": "WONTFIX",
						"keywords": "Security",
						"priority": "medium",
