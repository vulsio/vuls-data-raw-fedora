						"alias": "CVE-2018-14665",
						"creation_ts": "2018-10-10 02:20:52 +0000",
						"short_desc": "CVE-2018-14665 xorg-x11-server: Incorrect permission check in Xorg X server allows for privilege escalation",
						"delta_ts": "2022-03-13 15:43:23 +0000",
						"bug_status": "CLOSED",
						"resolution": "ERRATA",
						"keywords": "Security",
						"priority": "high",
						"bug_severity": "high",
						"depends_on": [
							"1639248",
							"1639249",
							"1643131",
							"1643132"
						],
						"blocked": [
							{
