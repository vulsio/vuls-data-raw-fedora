			"security": true,
			"title": "CVE-2018-13259 zsh: Improper handling of shebang line longer than 64 [fedora-all]",
			"bugzilla": {
				"bug_id": "1626185",
				"creation_ts": "2018-09-06 17:24:42 +0000",
				"short_desc": "CVE-2018-13259 zsh: Improper handling of shebang line longer than 64 [fedora-all]",
				"delta_ts": "2018-10-02 19:27:29 +0000",
