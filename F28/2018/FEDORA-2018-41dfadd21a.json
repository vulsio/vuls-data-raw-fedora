			"title": "CVE-2018-14526 wpa_supplicant: Unauthenticated EAPOL-Key decryption in wpa_supplicant [fedora-all]",
			"bugzilla": {
				"bug_id": "1614939",
				"creation_ts": "2018-08-10 19:32:05 +0000",
				"short_desc": "CVE-2018-14526 wpa_supplicant: Unauthenticated EAPOL-Key decryption in wpa_supplicant [fedora-all]",
				"delta_ts": "2018-08-23 09:43:40 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"bug_file_loc": "https://w1.fi/security/2018-1/",
