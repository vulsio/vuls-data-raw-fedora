								"who": {
									"text": "rschiron",
									"name": "Riccardo Schirone"
								},
								"bug_when": "2019-02-19 09:49:21 +0000",
								"thetext": "Reference:\nhttps://www.openwall.com/lists/oss-security/2019/02/18/3"
