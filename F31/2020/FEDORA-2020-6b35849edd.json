			"security": true,
			"title": "CVE-2020-15999 freetype: heap-based buffer overflow via malformed ttf files [fedora-all]",
			"bugzilla": {
				"bug_id": "1890211",
				"creation_ts": "2020-10-21 16:58:31 +0000",
				"short_desc": "CVE-2020-15999 freetype: heap-based buffer overflow via malformed ttf files [fedora-all]",
				"delta_ts": "2020-11-07 01:28:16 +0000",
