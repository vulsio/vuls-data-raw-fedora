			"title": "CVE-2020-16166 kernel: information exposure in drivers/char/random.c and kernel/time/timer.c",
			"bugzilla": {
				"bug_id": "1865751",
				"alias": "CVE-2020-16166",
				"creation_ts": "2020-08-04 04:23:16 +0000",
				"short_desc": "CVE-2020-16166 kernel: information exposure in drivers/char/random.c and kernel/time/timer.c",
				"delta_ts": "2023-12-15 18:41:29 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"keywords": "Security",
				"priority": "medium",
				"bug_severity": "medium",
