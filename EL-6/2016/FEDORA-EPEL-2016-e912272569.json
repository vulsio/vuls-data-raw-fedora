			"bugzilla": {
				"bug_id": "1323702",
				"alias": "CVE-2015-6360",
				"creation_ts": "2016-04-04 13:11:59 +0000",
				"short_desc": "CVE-2015-6360 libsrtp: improper handling of CSRC count and extension header length in RTP header",
				"delta_ts": "2020-10-29 18:15:35 +0000",
				"bug_status": "CLOSED",
				"resolution": "WONTFIX",
				"keywords": "Reopened, Security",
				"priority": "medium",
				"bug_severity": "medium",
				"depends_on": [
					"1323703",
					"1323704",
					"1323705"
				],
				"blocked": [
					{
						"bug_id": "1323706",
						"error": "NotPermitted"
					}
				],
				"external_bugs": {
