				"priority": "medium",
				"bug_severity": "medium",
				"blocked": [
					{
						"bug_id": "1037941",
						"alias": "CVE-2013-6824",
						"creation_ts": "2013-12-04 06:10:57 +0000",
						"short_desc": "CVE-2013-6824 zabbix: remote command execution from zabbix server",
						"delta_ts": "2019-09-29 13:10:38 +0000",
						"bug_status": "CLOSED",
						"resolution": "CURRENTRELEASE",
						"keywords": "Security",
						"priority": "medium",
						"bug_severity": "medium",
						"depends_on": [
							"1037942",
							"1037943",
							"1037944",
							"1082436"
						],
						"long_desc": [
							{
								"isprivate": "0",
								"commentid": "6583085",
								"comment_count": "0",
								"who": {
									"text": "ratulg",
									"name": "Ratul Gupta"
								},
								"bug_when": "2013-12-04 06:10:57 +0000",
								"thetext": "Zabbix agent is found to be vulnerable to remote command execution from the Zabbix server in some cases.\n\nIt is found that if a flexible user parameter is configured in the agent, including a newline in the parameters will execute newline section as a separate command even if UnsafeUserParameters are disabled.\n\nThis type of attack is known to be only possible from Zabbix server or Zabbix proxy systems that are explicitly allowed in the agent configuration. Only flexible user parameters are vulnerable, static ones are not.\n\nReferences:\nhttps://bugs.gentoo.org/show_bug.cgi?id=493250\nhttps://support.zabbix.com/browse/ZBX-7479"
							},
							{
								"isprivate": "0",
								"commentid": "6583097",
								"comment_count": "1",
								"who": {
									"text": "ratulg",
									"name": "Ratul Gupta"
								},
								"bug_when": "2013-12-04 06:13:56 +0000",
								"thetext": "\nCreated zabbix tracking bugs for this issue:\n\nAffects: fedora-all [bug 1037942]\nAffects: epel-all [bug 1037943]"
							},
							{
								"isprivate": "0",
								"commentid": "6583099",
								"comment_count": "2",
								"who": {
									"text": "ratulg",
									"name": "Ratul Gupta"
								},
								"bug_when": "2013-12-04 06:14:12 +0000",
								"thetext": "\nCreated zabbix20 tracking bugs for this issue:\n\nAffects: epel-all [bug 1037944]"
							}
						]
					}
				],
				"long_desc": [
					{
						"isprivate": "0",
						"commentid": "6583095",
