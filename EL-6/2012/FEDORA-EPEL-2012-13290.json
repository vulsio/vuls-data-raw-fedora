				"resolution": "ERRATA",
				"keywords": "Security, SecurityTracking",
				"priority": "urgent",
				"bug_severity": "urgent",
				"blocked": [
					{
						"bug_id": "869953",
						"alias": "CVE-2012-5671",
						"creation_ts": "2012-10-25 08:25:23 +0000",
						"short_desc": "CVE-2012-5671 exim: Heap-buffer overflow in DNS decode logic used for DKIM",
						"delta_ts": "2021-02-17 08:29:19 +0000",
						"bug_status": "CLOSED",
						"resolution": "ERRATA",
						"keywords": "Security",
						"priority": "urgent",
						"bug_severity": "urgent",
						"depends_on": [
							"870347",
