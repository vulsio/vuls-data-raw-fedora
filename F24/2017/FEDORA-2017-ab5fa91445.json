			"title": "CVE-2017-2619 samba: symlink race permits opening files outside share directory",
			"bugzilla": {
				"bug_id": "1429472",
				"alias": "CVE-2017-2619",
				"creation_ts": "2017-03-06 12:51:47 +0000",
				"short_desc": "CVE-2017-2619 samba: symlink race permits opening files outside share directory",
				"delta_ts": "2021-02-17 02:31:26 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"keywords": "Security",
				"priority": "medium",
				"bug_severity": "medium",
