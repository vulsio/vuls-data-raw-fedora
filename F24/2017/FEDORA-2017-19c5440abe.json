			"title": "CVE-2016-8745 tomcat: information disclosure due to incorrect Processor sharing",
			"bugzilla": {
				"bug_id": "1403824",
				"alias": "CVE-2016-8745",
				"creation_ts": "2016-12-12 12:42:45 +0000",
				"short_desc": "CVE-2016-8745 tomcat: information disclosure due to incorrect Processor sharing",
				"delta_ts": "2021-02-17 02:53:56 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"keywords": "Security",
				"priority": "high",
				"bug_severity": "high",
				"depends_on": [
					"1403825",
					"1413588",
					"1413589",
					"1413591",
					"1413592",
					"1424820",
					"1470474"
				],
				"blocked": [
					{
						"bug_id": "1403827",
						"error": "NotPermitted"
					},
					{
						"bug_id": "1428325",
						"error": "NotPermitted"
					}
