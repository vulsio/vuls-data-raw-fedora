					},
					{
						"isprivate": "0",
						"commentid": "10010776",
						"comment_count": "12",
						"who": {
							"text": "errata-xmlrpc",
							"name": "errata-xmlrpc"
						},
						"bug_when": "2016-12-21 12:13:22 +0000",
						"thetext": "This issue has been addressed in the following products:\n\n  Red Hat Enterprise Linux 6\n\nVia RHSA-2016:2974 https://rhn.redhat.com/errata/RHSA-2016-2974.html"
					},
