			"title": "CVE-2016-10002 squid: Information disclosure in HTTP request processing",
			"bugzilla": {
				"bug_id": "1405941",
				"alias": "CVE-2016-10002",
				"creation_ts": "2016-12-19 09:06:35 +0000",
				"short_desc": "CVE-2016-10002 squid: Information disclosure in HTTP request processing",
				"delta_ts": "2021-02-17 02:52:01 +0000",
				"bug_status": "CLOSED",
				"resolution": "ERRATA",
				"keywords": "Security",
				"priority": "medium",
				"bug_severity": "medium",
				"depends_on": [
					"1405944",
					"1412733",
					"1412734",
					"1412735",
					"1412736"
